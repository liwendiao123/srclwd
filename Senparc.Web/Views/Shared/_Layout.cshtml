@using System.Web.Mvc
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SenparcCoreFramework</title>
    <meta name="keywords" content="">
    <meta name="description" content="">

    <environment include="Development">
        <link href="~/bower_components/gentelella/vendors/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    </environment>
    <environment exclude="Development">
        <link href="~/bower_components/gentelella/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    </environment>
    @RenderSection("HeaderContent", false)
</head>
<body>
    <nav class="main-navigation">
        <div class="container">
            <div class="row">
                <div class=" ">
                    <nav class="navbar navbar-expand-lg navbar-light  navbar-paddimg-0">
                    </nav>
                </div>
            </div>
        </div>
    </nav>
    <!-- end navigation -->
    <!-- start content-->
    <section>
        <div class="container">
            @RenderBody()
        </div>
    </section>
    <!-- end content-->
    <!-- start footer-->
    <footer class="main-footer bg-gary"></footer>
    <div class=" uk-notify uk-notify-top-center"></div>
    <!-- end footer-->
    <environment include="Development">
        <script src="~/bower_components/jquery/dist/jquery.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
    </environment>
    @RenderSection("Scripts", required: false)
</body>
</html>
